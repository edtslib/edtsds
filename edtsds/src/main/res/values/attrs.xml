<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AlertBoxView">
        <attr name="type" format="enum">
            <enum name="success" value="0" />
            <enum name="warning" value="1" />
            <enum name="error" value="2" />
        </attr>
        <attr name="message" format="string" />
        <attr name="dismissible" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ButtonView">
        <attr name="variant" format="enum">
            <enum name="primary" value="0" />
            <enum name="secondary" value="1" />
            <enum name="outline" value="2" />
            <enum name="negative" value="3" />
        </attr>
        <attr name="size" format="enum">
            <enum name="medium" value="0" />
            <enum name="small" value="1" />
        </attr>
    </declare-styleable>
</resources>