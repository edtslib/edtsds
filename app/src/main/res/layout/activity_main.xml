<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_margin="@dimen/dimen_16dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:ignore="RtlSymmetry">
    <id.co.edtslib.edtsds.boarding.BoardingView
        android:id="@+id/boardingView"
        app:autoScrollInterval="5"
        app:canBackOnFirstPosition="true"
        app:circular="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        app:imageHeight="208dp"
        app:shape="@drawable/bg_onboarding_page_navigation"
        app:shapeSelectedWidth="@dimen/dimen_20dp"
        app:shapeSize="@dimen/dimen_8dp" />

</LinearLayout>